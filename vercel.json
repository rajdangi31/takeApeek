{
  "version": 2,
  "routes": [
    {
      "src":  "/api/send",          
      "methods": ["GET", "HEAD", "POST", "OPTIONS"],
      "dest": "/api/send.js",
      "sso": false
    },
    {
      "src": "/sw\\.js$",
      "headers": [
        { "key": "Service-Worker-Allowed", "value": "/" },
        { "key": "Cache-Control",        "value": "no-cache, no-store, must-revalidate" }
      ]
    }
  ]
}
